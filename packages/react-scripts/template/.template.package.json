{
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "classlist-polyfill": "^1.2.0",
    "core-js": "^3.0.1",
    "css-loader": "^2.1.1",
    "history": "^4.9.0",
    "husky": "^2.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.6",
    "lodash": "^4.17.11",
    "oidc-client": "^1.7.1",
    "prettier": "^1.17.0",
    "react-app-polyfill": "^1.0.1",
    "react-intl": "^2.8.0",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.13.2",
    "style-loader": "^0.23.1",
    "unorm": "^1.5.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "http-proxy-middleware": "^0.19.1",
    "license-checker": "^25.0.1",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.19.0",
    "react": "16.8.6",
    "react-app-rewired": "^2.1.3",
    "react-dom": "16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "source-map-explorer": "^1.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "concurrently --kill-others 'npm run-script dev-mock-server' 'react-app-rewired start --scripts-version rio-react-scripts'",
    "build": "react-app-rewired --max_old_space_size=4096 build --scripts-version rio-react-scripts",
    "test": "react-app-rewired test --scripts-version rio-react-scripts",
    "pretest": "npm run download-externals",
    "eject": "react-scripts eject",
    "check-licenses": "bash ./check_licenses.sh",
    "ci-install": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true npm install",
    "audit": "npm audit --registry https://registry.npmjs.org --parseable",
    "dev-mock-server": "nodenv __mocks__/rio-starter-devmocks.js --exec nodemon",
    "download-externals": "nodenv ./loadExternals.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "format-code": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  }
}
